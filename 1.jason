      "title": "Service Health Matrix",
      "transformations": [
        {
          "id": "merge",
          "options": {}
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "job": true,
              "instance": true,
              "__name__": true,
              "label_app_kubernetes_io_instance": true,
              "label_app_kubernetes_io_name": true,
              "label_app_kubernetes_io_version": true
            },
            "renameByName": {
              "deployment": "Service",
              "Value #B": "Available",
              "Value #C": "Desired",
              "Value #D": "CPU %",
              "Value #E": "Memory",
              "Value #F": "Restarts",
              "Value #G": "Uptime",
              "service": "Ingress",
              "Value #H": "RPS",
              "Value #I": "P95 Latency",
              "Value #J": "Error Rate"
            }
          }
        },
        {
          "id": "calculateField",
          "options": {
            "mode": "string",
            "reduce": {
              "reducer": "sum"
            },
            "replaceFields": false,
            "alias": "Pods",
            "binary": {
              "left": "Available",
              "operator": "/",
