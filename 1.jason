          "format": "table",
          "instant": true,
          "refId": "C"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
          },
          "editorMode": "code",
          "expr": "avg by (deployment) (rate(container_cpu_usage_seconds_total{namespace=\"$namespace\", pod=~\"(.*deployment.*|.*service.*)\", container!=\"\", container!=\"POD\"}[5m])) * 100",
          "format": "table",
          "instant": true,
          "refId": "D"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
          },
          "editorMode": "code",
          "expr": "sum by (deployment) (container_memory_working_set_bytes{namespace=\"$namespace\", pod=~\"(.*deployment.*|.*service.*)\", container!=\"\", container!=\"POD\"})",
          "format": "table",
          "instant": true,
          "refId": "E"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
          },
          "editorMode": "code",
          "expr": "sum by (deployment) (increase(kube_pod_container_status_restarts_total{namespace=\"$namespace\", pod=~\"(.*deployment.*|.*service.*)\"}[24h]))",
          "format": "table",
          "instant": true,
          "refId": "F"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
          },
          "editorMode": "code",
          "expr": "min by (deployment) (time() - kube_deployment_created{namespace=\"$namespace\", deployment=~\"$service\"})",
          "format": "table",
          "instant": true,
          "refId": "G"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
          },
          "editorMode": "code",
          "expr": "sum by (service) (rate(nginx_ingress_controller_requests{namespace=\"$namespace\"}[5m]))",
          "format": "table",
          "instant": true,
          "refId": "H"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
          },
          "editorMode": "code",
          "expr": "histogram_quantile(0.95, sum by (service, le) (rate(nginx_ingress_controller_request_duration_seconds_bucket{namespace=\"$namespace\"}[5m]))) * 1000",
          "format": "table",
          "instant": true,
          "refId": "I"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
          },
          "editorMode": "code",
          "expr": "sum by (service) (rate(nginx_ingress_controller_requests{namespace=\"$namespace\", status=~\"5..\"}[5m])) / sum by (service) (rate(nginx_ingress_controller_requests{namespace=\"$namespace\"}[5m])) * 100",
          "format": "table",
          "instant": true,
          "refId": "J"
        }
